<div class="thumbnail">
        <%= link_to image_tag(item.item_images.first.picture_url ,:class => 'img'), item_path(item) %>
        <div class="caption">
            <h3><%= item.title %></h3>
            Price: <span><%= item.price %></span>
            <p><a href="#" class="btn btn-primary" role="button">Add to Cart</a> 
            
        </div>
</div>









   