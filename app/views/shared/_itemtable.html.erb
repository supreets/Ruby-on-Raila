<div class = "col-md-10">
<% @items.in_groups_of(4, false).each do |group| %>
  <div class='row'>
    <% group.each do |item| %>
      <div class='col-lg-3 col-sm-4 col-xs-6'>
          <%= render item %>
      </div>
     <% end %>
    </div>
<% end %>
</div>
